<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox fx:id="mainContainer" prefHeight="600.0" prefWidth="1000.0">
               <children>
                  <VBox fx:id="statsContainer" prefHeight="200.0" prefWidth="300.0" stylesheets="@stylesheet.css">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Statistics" wrappingWidth="280.0">
                           <font>
                              <Font size="26.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="20.0" left="10.0" top="20.0" />
                           </VBox.margin>
                        </Text>
                        <VBox prefWidth="100.0">
                           <children>
                              <Text fx:id="algorithmTypeText" onMouseDragOver="#update" strokeType="OUTSIDE" strokeWidth="0.0" text="Algorithm Type:" wrappingWidth="280.0">
                                 <font>
                                    <Font size="19.0" />
                                 </font>
                              </Text>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Time Elapsed:" wrappingWidth="280.0">
                                 <font>
                                    <Font size="19.0" />
                                 </font>
                              </Text>
                           </children>
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </VBox>
                        <VBox prefWidth="100.0">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Number of processors:" wrappingWidth="280.0">
                                 <font>
                                    <Font size="19.0" />
                                 </font>
                              </Text>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Parallel processing threads:" wrappingWidth="280.0">
                                 <font>
                                    <Font size="19.0" />
                                 </font>
                              </Text>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Memory used:" wrappingWidth="280.0">
                                 <font>
                                    <Font size="19.0" />
                                 </font>
                              </Text>
                           </children>
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </VBox>
                        <VBox prefWidth="100.0">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Branches bounded:" wrappingWidth="280.0">
                                 <font>
                                    <Font size="19.0" />
                                 </font>
                              </Text>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Branches pruned:" wrappingWidth="280.0">
                                 <font>
                                    <Font size="19.0" />
                                 </font>
                              </Text>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="States generated:" wrappingWidth="280.0">
                                 <font>
                                    <Font size="19.0" />
                                 </font>
                              </Text>
                           </children>
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </VBox>
                     </children>
                  </VBox>
                  <TabPane fx:id="visualsContainer" prefHeight="600.0" prefWidth="700.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                      <Tab fx:id="graphTab" text="Search Graph">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                        </content>
                      </Tab>
                      <Tab fx:id="taskTab" text="Task Schedule">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                        </content>
                      </Tab>
                        <Tab fx:id="resultTab" text="Schedule Results">
                           <content>
                              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                           </content>
                        </Tab>
                    </tabs>
                  </TabPane>
               </children>
            </HBox>
         </children>
    </AnchorPane>
  </children>
</VBox>
